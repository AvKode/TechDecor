@model IEnumerable<PAKTD.Models.MO.mCliente>
@{
    ViewBag.Title = "ListarFuncionario";
    Layout = "~/Views/Dashboard/_Layout.cshtml";
}


<h2 class="text-center">CLIENTES CADASTRADOS</h2>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr class="bg-success">
            <th>Nome</th>
            <th>Telefone</th>
            <th>Email</th>
            <th>CPF</th>
            <th>CEP</th>

        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {

            foreach (var cliente in Model)
            {
                <tr>

                    <td>@cliente.NmCli</td>
                    <td>@cliente.FoneCli</td>
                    <td>@cliente.EmailCli</td>
                    <td>@cliente.CpfCli</td>
                    <td>@cliente.CepCli</td>

                </tr>
            };
        }
        else
        {

            <tr>
                <td class="text-center" colspan="5">Nenhum cliente cadastrado.</td>
            </tr>

        }

    </tbody>

</table>



